<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}" plugins-version="2">

    <plugin-info>
        <description>${project.description}</description>
        <version>${project.version}</version>
        <vendor name="${project.organization.name}" url="${project.organization.url}" />
        <param name="plugin-icon">images/pluginIcon.png</param>
        <param name="plugin-logo">images/pluginLogo.png</param>
    </plugin-info>

<web-panel key="simple-summary-input" location="webpanels.admin.summary.right-panels" weight="100">
    <description>A simple input field for project configuration summary.</description>
    <label key = "database selection" />
    <resource name="view" type="velocity" location="/templates/custom-summary-field.vm"/>
</web-panel>

    <web-panel key="feedback-panel" location="atl.jira.view.issue.right.context" weight="100" name="Feedback Panel">
    <description>Displays a list of feedback items.</description>
    <context-provider class="com.yourcompany.FeedbackContextProvider"/>
    <resource name="view" type="velocity" location="templates/feedback-panel.vm"/>
</web-panel>
 

</atlassian-plugin>
